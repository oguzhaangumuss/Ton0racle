{
  "name": "ton-oracle",
  "version": "1.0.0",
  "description": "Multi-source Oracle system for TON blockchain with extensible architecture",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "deploy:testnet": "npm run build && node dist/deploy.js testnet",
    "deploy:mainnet": "npm run build && node dist/deploy.js mainnet"
  },
  "dependencies": {
    "@ton/ton": "^15.0.0",
    "@ton/core": "^0.60.0",
    "@pythnetwork/pyth-ton-js": "^1.0.0",
    "@pythnetwork/hermes-client": "^1.0.0",
    "tonweb": "^0.0.66",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/node-cron": "^3.0.11",
    "@types/jest": "^29.5.12",
    "typescript": "^5.4.2",
    "tsx": "^4.7.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "eslint": "^8.57.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0"
  },
  "keywords": [
    "ton",
    "blockchain", 
    "oracle",
    "price-feed",
    "defi",
    "cryptocurrency",
    "typescript",
    "smart-contracts"
